{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "react/prop-types": "off", // ğŸ‘ˆ Disable prop-types validation
    "no-unused-vars": "warn", // ğŸ‘ˆ Keep unused vars as warnings
    "react/jsx-key": "error", // ğŸ‘ˆ Keep key requirement as error
    "react/jsx-no-undef": "error" // ğŸ‘ˆ Keep undefined component check
  }
}
